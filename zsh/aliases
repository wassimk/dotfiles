# Set personal aliases, overriding those provided by oh-my-zsh libs
# plugins, and themes. For a full list of active aliases, run `alias`.
alias ll='ls -lhF'    # Pretty listing
alias la='ls -lahF'    # Show all files
alias lh='ls -ldhF .*'  # Show dot files / folders
alias l='ll'          # Override oh-my-zsh default
alias lsa='la'          # Override oh-my-zsh default

# Reload zsh
alias zs='source ~/.zshrc'

###
# tmux
###
alias tml='tmux list-sessions'
alias tmka='tmux kill-server'
